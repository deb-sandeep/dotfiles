{
    "table-variate-frequecy-5-columns": {
        "prefix": "tab-var-freq-5",
        "body": [
            "\\begin{table}[h]",
            "\\centering",
            "\\renewcommand{\\arraystretch}{1.3}",
            "\\begin{tabularx}{0.5\\textwidth}{| p {2.75 cm} | X | X | X | X | X | }",
            "    \\hline",
            "    Variate (x) & $1 & $2 & $3 & $4 & $5 \\\\\\",
            "    \\hline",
            "    Frequency (f) & $6 & $7 & $8 & $9 & $10 \\\\\\",
            "    \\hline",
            "\\end{tabularx}",
            "\\end{table}",
            "$0"
        ],
        "description": "Variate, Frequenty horizational table with 5 columns."
    },
    "matrix": {
        "prefix": "mat",
        "body": [
            "\\begin{bmatrix*} $1 \\\\\\ $2 \\end{bmatrix*}$0"
        ],
        "description": "2x2 Matrix"
    },
    "cos" : {
        "prefix" : "cos",
        "body" : [ "\\cos$1 \\theta$0" ],
        "description": "Cos theta"
    },
    "sin" : {
        "prefix" : "sin",
        "body" : [ "\\sin$1 \\theta$0" ],
        "description": "Sin theta"
    },
    "tan" : {
        "prefix" : "tan",
        "body" : [ "\\tan$1 \\theta$0" ],
        "description": "Tin theta"
    },
    "sec" : {
        "prefix" : "sec",
        "body" : [ "\\sec$1 \\theta$0" ],
        "description": "Sec theta"
    },
    "csc" : {
        "prefix" : "csc",
        "body" : [ "\\mathrm{cosec}$1 \\ \\theta$0" ],
        "description": "Cosec theta"
    },
    "cot" : {
        "prefix" : "cot",
        "body" : [ "\\cot$1 \\theta$0" ],
        "description": "Cot theta"
    },
    "blank" : {
        "prefix" : "blank",
        "body" : [ "\\rule{1.5cm}{0.15mm}" ],
        "description": "Blank for FIB"
    },
    "table-class-frequency" : {
        "prefix" : "table-class-frequency",
        "body" : [
            "\\begin{table}[h]",
            "\\centering",
            "\\renewcommand{\\arraystretch}{1.3}",
            "\\begin{tabularx}{0.5\\textwidth}{| p {3 cm} | X | }",
            "    \\hline",
            "    \\rowcolor{lightgray!30} ${1:Class} & ${2:Frequency} \\\\\\",
            "    \\hline",
            "    \\\\\\",
            "    \\hline",
            "    \\\\\\",
            "    \\hline",
            "    \\\\\\",
            "    \\hline",
            "    \\\\\\",
            "    \\hline",
            "    \\\\\\",
            "    \\hline",
            "\\end{tabularx}",
            "\\end{table}"
        ],
        "description" : "A class frequency table."
    },
    "assertion-reason" : {
        "prefix" : "ass-res",
        "body" : [
            "\\item Choose the right option: \\hfill [4]",
            "",
            "    \\textbf{Assertion:} ",
            "",
            "    \\textbf{Reason:} ",
            "",
            "    \\begin{enumerate}[label={\\alph*.}]",
            "    \\setlength\\itemsep{0em}",
            "        \\item Both assertion and reason are correct and reason is the",
            "            correct explanation of assertion.",
            "        \\item Both assertion and reason are correct but reason is not",
            "            the correct explanation of assertion.",
            "        \\item Assertion is correct but reason is not correct.",
            "        \\item Assertion is incorrect but reason is correct.",
            "    \\end{enumerate}",
            "",
            ""
        ],
        "description" : "Assertion reason template"
    }
}
